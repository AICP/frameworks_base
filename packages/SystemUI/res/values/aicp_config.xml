<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>

    <!-- The default tiles to display in QuickSettings -->
    <string name="quick_settings_tiles_extra" translatable="false">
        compass,music,screenshot,reboot,usb_tether,caffeine,cpuinfo,aicp_extras,heads_up,smartpixels,aod,locale,sound,volume_panel,dataswitch,sync,onthego,soundsearch
    </string>

    <!-- The CPU temperature sensor path, defaults to empty -->
    <string name="config_cpuTempSensor" translatable="false"></string>

    <!-- The CPU temperature divider, if needed -->
    <integer name="config_cpuTempDivider" translatable="false">1</integer>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">5</integer>
    <integer name="config_maxVisibleNotificationIconsOnLock">6</integer>

    <!-- On-The-Go -->
    <integer name="quick_settings_onthego_dialog_short_timeout">3000</integer>
    <integer name="quick_settings_onthego_dialog_long_timeout">6000</integer>
</resources>
